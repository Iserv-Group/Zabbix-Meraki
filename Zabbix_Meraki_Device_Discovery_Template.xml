                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>meraki.sh[network.uplink.data,{$NETWORK_ID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Uplink Interface Status Check</name>
                            <type>DEPENDENT</type>
                            <key>wan.uplink.status[{#SERIAL},UplinkStatusCheck]</key>
                            <delay>0</delay>
                            <history>30d</history>
                            <applications>
                                <application>
                                    <name>WAN</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>Meraki Interface Status</name>
                            </valuemap>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$[?(@.serial == '{#SERIAL}')].uplinks</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.*.*</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$[?(@.interface == '{#INT_NAME}')].status</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>STR_REPLACE</type>
                                    <parameters>
                                        <parameter>[&quot;active&quot;]</parameter>
                                        <parameter>3</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>STR_REPLACE</type>
                                    <parameters>
                                        <parameter>[&quot;ready&quot;]</parameter>
                                        <parameter>2</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>STR_REPLACE</type>
                                    <parameters>
                                        <parameter>[&quot;failed&quot;]</parameter>
                                        <parameter>1</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>STR_REPLACE</type>
                                    <parameters>
                                        <parameter>[&quot;not connected&quot;]</parameter>
                                        <parameter>0</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>meraki.sh[network.uplink.data,{$NETWORK_ID}]</key>
                            </master_item>
