                                <key>meraki.sh[network.uplink.data,{$NETWORK_ID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Interface Signal Strength (RSRQ)</name>
                            <type>DEPENDENT</type>
                            <key>wan.uplink.status[{#SERIAL},RSRQ]</key>
                            <delay>0</delay>
                            <value_type>FLOAT</value_type>
                            <units>dB</units>
                            <applications>
                                <application>
                                    <name>WAN</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$[?(@.serial == '{#SERIAL}')].uplinks</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.*.*</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
