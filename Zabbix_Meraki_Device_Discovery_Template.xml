    <templates>
        <template>
            <template>Meraki Device Discovery</template>
            <name>Meraki Device Discovery</name>
            <description>Template used to create Zabbix items and triggers for all devices on a given Meraki Network. Data Pulled automatically from Dashboard API export.</description>
            <groups>
                <group>
                    <name>Templates/Applications</name>
                </group>
                <group>
                    <name>Templates/Network devices</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Device</name>
                </application>
                <application>
                    <name>Network</name>
                </application>
                <application>
                    <name>WAN</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>data pull - device discovery</name>
                    <type>EXTERNAL</type>
                    <key>meraki.sh[network.data,{$NETWORK_ID}]</key>
                    <delay>2h</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Device</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>data pull - uplink quickpoll</name>
                    <type>EXTERNAL</type>
                    <key>meraki.sh[network.uplink.data,{$NETWORK_ID}]</key>
                    <delay>5m</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>WAN</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>Discover Meraki Devices</name>
                    <type>EXTERNAL</type>
                    <key>meraki.sh[device.discovery, {$NETWORK_ID}]</key>
                    <delay>2h</delay>
                    <lifetime>1h</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Device Address</name>
                            <type>DEPENDENT</type>
                            <key>device.status[{#SERIAL},ADDRESS]</key>
                            <delay>0</delay>
                            <history>2w</history>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Device</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$[?(@.serial == '{#SERIAL}')].address</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>TRIM</type>
                                    <parameters>
                                        <parameter>[&quot;]</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>meraki.sh[network.data,{$NETWORK_ID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Device MAC</name>
                            <type>DEPENDENT</type>
                            <key>device.status[{#SERIAL},MAC]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Device</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$[?(@.serial == '{#SERIAL}')].mac</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>TRIM</type>
                                    <parameters>
                                        <parameter>[&quot;]</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>meraki.sh[network.data,{$NETWORK_ID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Network ID</name>
                            <type>DEPENDENT</type>
                            <key>device.status[{#SERIAL},NETWORKID]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Network</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$[?(@.serial == '{#SERIAL}')].networkId</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>TRIM</type>
                                    <parameters>
                                        <parameter>[&quot;]</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>meraki.sh[network.data,{$NETWORK_ID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Device Notes</name>
                            <type>DEPENDENT</type>
                            <key>device.status[{#SERIAL},NOTES]</key>
                            <delay>0</delay>
                            <history>2w</history>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Device</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$[?(@.serial == '{#SERIAL}')].notes</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>TRIM</type>
                                    <parameters>
                                        <parameter>[&quot;]</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>meraki.sh[network.data,{$NETWORK_ID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Device Serial Number</name>
                            <type>DEPENDENT</type>
                            <key>device.status[{#SERIAL},SERIAL]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Device</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$[?(@.serial == '{#SERIAL}')].serial</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>TRIM</type>
                                    <parameters>
                                        <parameter>[&quot;]</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>meraki.sh[network.data,{$NETWORK_ID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Device Tags</name>
                            <type>DEPENDENT</type>
                            <key>device.status[{#SERIAL},TAGS]</key>
                            <delay>0</delay>
                            <history>2w</history>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Device</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$[?(@.serial == '{#SERIAL}')].tags</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>TRIM</type>
                                    <parameters>
                                        <parameter>[&quot;]</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>meraki.sh[network.data,{$NETWORK_ID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Device Status Check</name>
                            <type>DEPENDENT</type>
                            <key>device.status[{#SERIAL}]</key>
                            <delay>0</delay>
                            <applications>
                                <application>
                                    <name>Device</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>Meraki Device Status</name>
                            </valuemap>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$[?(@.serial == '{#SERIAL}')].status</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>STR_REPLACE</type>
                                    <parameters>
                                        <parameter>[&quot;online&quot;]</parameter>
                                        <parameter>2</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>STR_REPLACE</type>
                                    <parameters>
                                        <parameter>[&quot;alerting&quot;]</parameter>
                                        <parameter>1</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>STR_REPLACE</type>
                                    <parameters>
                                        <parameter>[&quot;offline&quot;]</parameter>
                                        <parameter>0</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>meraki.sh[network.uplink.data,{$NETWORK_ID}]</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{max(6m)}=0</expression>
                                    <name>{#NAME} Unreachable for 5 Minute</name>
                                    <status>DISABLED</status>
                                    <discover>NO_DISCOVER</discover>
                                    <priority>INFO</priority>
                                    <description>The following device has been unreachable from the Meraki cloud for 5 min
&lt;b&gt;Meraki Org and Network Name:&lt;/b&gt;  &lt;i&gt;{HOST.NAME}&lt;/i&gt;
&lt;b&gt;Meraki Device Name:&lt;/b&gt;  &lt;i&gt;{#NAME}&lt;/i&gt;</description>
                                    <dependencies>
                                        <dependency>
                                            <name>{#NAME} Unreachable for 10 Minute</name>
                                            <expression>{Meraki Device Discovery:device.status[{#SERIAL}].max(11m)}=0</expression>
                                        </dependency>
                                        <dependency>
                                            <name>{#NAME} Unreachable for 15 Minute</name>
                                            <expression>{Meraki Device Discovery:device.status[{#SERIAL}].max(16m)}=0</expression>
                                        </dependency>
                                    </dependencies>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{max(11m)}=0</expression>
                                    <name>{#NAME} Unreachable for 10 Minute</name>
                                    <status>DISABLED</status>
                                    <discover>NO_DISCOVER</discover>
                                    <priority>WARNING</priority>
                                    <description>The following device has been unreachable from the Meraki cloud for 10 min
&lt;b&gt;Meraki Org and Network Name:&lt;/b&gt;  &lt;i&gt;{HOST.NAME}&lt;/i&gt;
&lt;b&gt;Meraki Device Name:&lt;/b&gt;  &lt;i&gt;{#NAME}&lt;/i&gt;</description>
                                    <dependencies>
                                        <dependency>
                                            <name>{#NAME} Unreachable for 15 Minute</name>
                                            <expression>{Meraki Device Discovery:device.status[{#SERIAL}].max(16m)}=0</expression>
                                        </dependency>
                                    </dependencies>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{max(16m)}=0</expression>
                                    <name>{#NAME} Unreachable for 15 Minute</name>
                                    <status>DISABLED</status>
                                    <discover>NO_DISCOVER</discover>
                                    <priority>AVERAGE</priority>
                                    <description>The following device has been unreachable from the Meraki cloud for 15 min
&lt;b&gt;Meraki Org and Network Name:&lt;/b&gt;  &lt;i&gt;{HOST.NAME}&lt;/i&gt;
&lt;b&gt;Meraki Device Name:&lt;/b&gt;  &lt;i&gt;{#NAME}&lt;/i&gt;</description>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Discovered WAN Connections</name>
                    <type>EXTERNAL</type>
                    <key>meraki.sh[wan.discovery,{$NETWORK_ID}]</key>
                    <delay>1h</delay>
                    <lifetime>7d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Interface Signal Type (3G-4G)</name>
                            <type>DEPENDENT</type>
                            <key>wan.uplink.status[{#SERIAL},ConnectionType]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>WAN</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$[?(@.serial == '{#SERIAL}')].uplinks</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.*.*</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$[?(@.interface == &quot;cellular&quot;)].connectionType</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>TRIM</type>
                                    <parameters>
                                        <parameter>[&quot;]</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>meraki.sh[network.uplink.data,{$NETWORK_ID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Uplink Interface Type</name>
                            <type>DEPENDENT</type>
                            <key>wan.uplink.status[{#SERIAL},InterfaceType]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <applications>
                                <application>
                                    <name>WAN</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$[?(@.serial == '{#SERIAL}')].uplink</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>TRIM</type>
                                    <parameters>
                                        <parameter>[&quot;]</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>meraki.sh[network.uplink.data,{$NETWORK_ID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Interface Latency</name>
                            <type>DEPENDENT</type>
                            <key>wan.uplink.status[{#SERIAL},Latency]</key>
                            <delay>0</delay>
                            <value_type>FLOAT</value_type>
                            <units>ms</units>
                            <applications>
                                <application>
                                    <name>WAN</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.[?(@.serial == '{#SERIAL}')].timeSeries</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$..latencyMs</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>TRIM</type>
                                    <parameters>
                                        <parameter>[&quot;]</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>meraki.sh[network.uplink.data,{$NETWORK_ID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Interface Packet Loss</name>
                            <type>DEPENDENT</type>
                            <key>wan.uplink.status[{#SERIAL},PacketLoss]</key>
                            <delay>0</delay>
                            <value_type>FLOAT</value_type>
                            <units>%</units>
                            <applications>
                                <application>
                                    <name>WAN</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.[?(@.serial == '{#SERIAL}')].timeSeries</parameter>
